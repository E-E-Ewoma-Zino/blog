	<!-- partials -->
	<!-- partials: partials/_header -->
	<%- include("partials/_header") %>
		<!-- partials -->
		<!-- partials: partials/_nav -->
		<%- include("partials/_nav") %>

			<section class="">
				<div class="container-fluid h-custom">
					<div class="row d-flex justify-content-center align-items-center h-100">
						<div class="col-md-9 col-lg-6 col-xl-5">
							<img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-login-form/draw2.webp"
								class="img-fluid" alt="Sample image">
						</div>
						<div id="login" class="col-md-8 col-lg-6 col-xl-4 offset-xl-1">
							<form action="/auth/login">
								<div
									class="d-none flex-row align-items-center justify-content-center justify-content-lg-start">
									<p class="lead fw-normal mb-0 me-3">Sign in with</p>
									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-facebook-f"></i>
									</button>

									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-twitter"></i>
									</button>

									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-linkedin-in"></i>
									</button>
								</div>

								<div class="divider d-flex align-items-center my-4">
									<!-- <p class="text-center fw-bold mx-3 mb-0">Or</p> -->
									<div class="brand-logo" style="font-weight: bolder;">
										<img src="/admin/images/logo.png" class="img-fluid rounded-circle"
											style="width: 3rem;" alt="logo">
										Global Finance
									</div>
								</div>

								<!-- Email input -->
								<div class="form-outline mb-4">
									<label class="form-label" for="form3Example3">Email address</label>
									<input type="email" id="form3Example3" name="email" class="form-control form-control-lg"
										placeholder="Enter a valid email address" />
								</div>

								<!-- Password input -->
								<div class="form-outline mb-3">
									<label class="form-label" for="form3Example4">Password</label>
									<input type="password" id="form3Example4" name="password" class="form-control form-control-lg"
										placeholder="Enter password" />
								</div>

								<div class="d-none justify-content-between align-items-center">
									<!-- Checkbox -->
									<div class="form-check mb-0">
										<!-- <input class="form-check-input me-2" type="checkbox" value=""
											id="form2Example3" />
										<label class="form-check-label" for="form2Example3">
											Remember me
										</label> -->
									</div>
									<a href="#!" class="text-body">Forgot password?</a>
								</div>

								<div class="text-center text-lg-start mt-4 pt-2">
									<button type="submit" class="btn primary-bg text-white btn-lg"
										style="padding-left: 2.5rem; padding-right: 2.5rem;">Login</button>
									<p class="small fw-bold mt-2 pt-1 mb-0">Don't have an account? <a href="#x"
											id="openRegister" class="link-danger">Register</a></p>
								</div>

							</form>
						</div>
						<!-- register -->
						<div id="register" class="col-md-8 col-lg-6 col-xl-4 offset-xl-1">
							<form action="/auth/register">
								<div
									class="d-none flex-row align-items-center justify-content-center justify-content-lg-start">
									<p class="lead fw-normal mb-0 me-3">Sign in with</p>
									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-facebook-f"></i>
									</button>

									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-twitter"></i>
									</button>

									<button type="button" class="btn btn-primary btn-floating mx-1">
										<i class="fab fa-linkedin-in"></i>
									</button>
								</div>

								<div class="divider d-flex align-items-center my-4">
									<!-- <p class="text-center fw-bold mx-3 mb-0">Or</p> -->
									<div class="brand-logo" style="font-weight: bolder;">
										<img src="/admin/images/logo.png" class="img-fluid rounded-circle"
											style="width: 3rem;" alt="logo">
										Global Finance
									</div>
								</div>

								<!-- Username input -->
								<div class="form-outline mb-4">
									<label class="form-label" for="form3Example3">Username</label>
									<input type="text" id="form3Example3" name="username" class="form-control form-control-lg"
										placeholder="Enter a username" />
								</div>

								<!-- Email input -->
								<div class="form-outline mb-4">
									<label class="form-label" for="form3Example3">Email address</label>
									<input type="email" id="form3Example3" name="email" class="form-control form-control-lg"
										placeholder="Enter a valid email address" />
								</div>

								<!-- Password input -->
								<div class="form-outline mb-3">
									<label class="form-label" for="form3Example4">Password</label>
									<input type="password" id="form3Example4" name="password" class="form-control form-control-lg"
										placeholder="Enter password" />
								</div>

								<div class="d-none justify-content-between align-items-center">
									<!-- Checkbox -->
									<div class="form-check mb-0">
										<!-- <input class="form-check-input me-2" type="checkbox" value=""
											id="form2Example3" />
										<label class="form-check-label" for="form2Example3">
											Remember me
										</label> -->
									</div>
									<a href="#!" class="text-body">Forgot password?</a>
								</div>

								<div class="text-center text-lg-start mt-4 pt-2">
									<button type="submit" class="btn primary-bg text-white btn-lg"
										style="padding-left: 2.5rem; padding-right: 2.5rem;">Register</button>
									<p class="small fw-bold mt-2 pt-1 mb-0">Already have an account? <a href="#x"
											id="openLogin" class="link-danger">Login</a></p>
								</div>

							</form>
						</div>
					</div>
				</div>
			</section>

